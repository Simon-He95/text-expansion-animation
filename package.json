{
  "name": "text-expansion-animation",
  "version": "0.0.8",
  "packageManager": "pnpm@7.33.7",
  "description": "text-expansion-animation",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/text-expansion-animation.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/text-expansion-animation/issues"
  },
  "keywords": [
    "text-expansion-animation",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./style.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.6"
  },
  "scripts": {
    "build": "esno ./build.ts",
    "dev": "npm run build -- --watch src",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest --dir test -u",
    "ptest": "pnpm run -C playground test -u",
    "preview": "pnpm run -C playground preview",
    "test:e2e": "cypress open",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && git push origin --tags -f && npm publish"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.0.0",
    "@types/node": "^18.19.71",
    "@vitejs/plugin-vue": "^3.2.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.57.1",
    "eslint-plugin-n": "^15.7.0",
    "esno": "^4.8.0",
    "lazy-js-utils": "^0.1.28",
    "prettier": "^2.8.8",
    "text-expansion-animation": "workspace:^",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.9",
    "vitest": "^0.13.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
